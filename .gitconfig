[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[user]
	name = Robert Wilkinson
	email = wilkinson.robert.a@gmail.com
[alias]
	st = status
	br = branch
	co = checkout
	lp = log -p
  cm = commit -m
[core]
	editor = nvim
  excludesfile = ~/.gitignore
[push]
	default = simple
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
